  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <CodeAnalysisRuleSet>../../Analysers.ruleset</CodeAnalysisRuleSet>
    <RootNamespace>Enable.HealthCheck</RootNamespace>
    <AssemblyName>Enable.HealthCheck.Abstractions</AssemblyName>
    <PackageId>Enable.HealthCheck.Abstractions</PackageId>
    <Title>Enable ASP.NET Core Health Check Abstractions</Title>
    <Description>Abstractions to support web application URL health monitoring.</Description>
    <Authors>Enable Â· enable.com</Authors>
    <PackageProjectUrl>https://github.com/EnableSoftware/Enable.HealthCheck</PackageProjectUrl>
